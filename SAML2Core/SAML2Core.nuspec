<?xml version="1.0"?>
<package >
  <metadata>
    <id>Saml2Core</id>
    <version>2.0.0.0</version>
    <title>Saml2 for .Net Core</title>
    <authors>Dina Heidar</authors>
    <owners>Office Of Technology Serives, State of Louisiana</owners>
    <language>en-US</language>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://mit-license.org/</licenseUrl>   
    <description>ASP.NET Core middleware that enables an application to support the SAML2 SSO authentication workflow. This middleware is opensource (free to use) and is not dependent on any .NET Framework libraries. It has been tested with ADFS and IdentityServer4 as well.</description>
    <releaseNotes>New updates and changes to package.</releaseNotes>
    <copyright>Copyright 2019</copyright>
    <tags>saml .net core adfs authentication</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Microsoft.AspNetCore.Authentication" version="2.1.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.Authentication.Cookies" version="2.1.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.IdentityModel.Protocols" version="5.2.4" exclude="Build,Analyzers" />
        <dependency id="Microsoft.IdentityModel.Tokens.Saml" version="5.2.4" exclude="Build,Analyzers" />
        <dependency id="Microsoft.IdentityModel.Xml" version="5.2.4" exclude="Build,Analyzers" />        
      </group>
    </dependencies>
  </metadata>
  <files>   
    <file src="bin\Debug\netstandard2.0\*.dll" target="lib\netstandard2.0" />
  </files>
</package>